# Backend Environment Variables
# Copy this file to .env and fill in your values

# Hugging Face API Key for welcome message translation
HF_API_KEY=your_huggingface_api_key_here
# Optional: override default HF model used by scribe
HF_MODEL=moonshotai/Kimi-K2-Instruct:novita

# SMTP configuration for email updates (set your provider credentials)
SMTP_HOST=
SMTP_PORT=587
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_FROM_EMAIL=

# Whistleblower tool configuration
AG2_API_KEY=
HF_INFERENCE_API_KEY=
WHISTLEBLOWER_RATE_LIMIT=60