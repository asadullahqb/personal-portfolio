{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "@next_public_backend_url"
  },
  "domains": ["asadullahqamarbhatti.com"],
  "github": {
    "enabled": false
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_BACKEND_URL": "@next_public_backend_url"
    }
  }
}